{
    "manifest_version": 3,
    "name": "GitHub Multi-Select Delete",
    "description": "Adds checkboxes for multi-select and delete on GitHub repo pages.",
    "version": "1.0.0",
    "content_scripts": [
      {
        "matches": ["https://github.com/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "storage"]
  }
  